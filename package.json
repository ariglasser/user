{
  "name": "pwa-auth",
  "author": "Jack Markiewicz <jack.solutionbox@gmail.com>(https://www.contractorscentre.com)",
  "version": "0.0.2",
  "description": " PWA Web Components with Firebase Authentication, lit-Element + Redux. An Icon Button. A drop-down Drawer and Settings page. supplied with common Firebase Authentication Functions.",
  "main": "index.js",
  "module": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/busynest/user"
  },
  "keywords": [
    "lit-html",
    "redux",
    "firebase",
    "user",
    "management",
    "drawer",
    "settings"
  ],
  "license": "BSD-3-Clause",
  "flat": true,
  "scripts": {
    "start": "polymer serve",
    "build": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled"
  },
  "dependencies": {
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "polymer-cli": "^1.9.3",
    "firebase-tools": "^6.5.0"
  }
}
